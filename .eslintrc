{
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "jsx-a11y/img-has-alt":0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true
      }
    ],
    "prefer-rest-params": 0,
    "react/forbid-prop-types": 0,
    "react/no-unescaped-entities": 0
  }
}
